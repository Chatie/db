  "bugs": {
    "url": "https://github.com/chatie/db/issues"
  },
  "homepage": "https://github.com/chatie/db#readme",
  "devDependencies": {
    "@types/node": "^7.0.5",
    "brolog": "^1.1.18",
    "eslint": "^4.17.0",
    "git-scripts": "^0.2.1",
    "shx": "^0.2.2",
    "ts-node": "^2.1.0",
    "tslint": "^5.9.1",
    "tslint-jsdoc-rules": "^0.1.2",
    "typescript": "^2.7.1"
  },
  "peerDependencies": {
    "brolog": ">=1"
  },
