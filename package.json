{
  "name": "@chatie/db",
  "version": "0.5.23",
  "description": "Database for Chatie",
  "scripts": {
    "clean": "shx rm -fr dist",
    "dist": "npm run clean && tsc",
    "generate-schema": "ts-node scripts/generate-schema",
    "graphcool:restart": "chatie-graphql-local-stop && chatie-graphql-local-start",
    "lint": "npm run lint:ts",
    "lint:ts": "echo tslint v`tslint --version` && tslint --project tsconfig.json --exclude \"tests/fixtures/**\" && tsc --noEmit",
    "ng:packagr": "ng-packagr -p package.json",
    "test": "npm run lint && npm run test:unit",
    "test:debug": "BROLOG_LEVEL=silly blue-tape -r ts-node/register -r source-map-support/register src/hostie/*.spec.ts",
    "test:pack": "npm run dist && npm pack && export TMPDIR=/tmp/pack-testing.$$ && mkdir $TMPDIR && mv *-*.*.*.tgz $TMPDIR && cp tests/fixtures/pack-testing.js $TMPDIR && cd $TMPDIR && npm init -y && (for i in {1..3}; do npm i *-*.*.*.tgz @angular/core auth-angular brolog rxjs state-switch && ./node_modules/.bin/chatie-graphql-local-restart && node --harmony_async_iteration pack-testing.js && break || sleep 1; done)",
    "test:unit": "blue-tape -r ts-node/register {src,tests}/**/*.spec.ts"
  },
  "git": {
    "scripts": {
      "pre-push": "bash ./scripts/pre-push.sh"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatie/db.git"
  },
  "keywords": [
    "chatie",
    "database"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/chatie/db/issues"
  },
  "homepage": "https://github.com/chatie/db#readme",
  "devDependencies": {
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@auth0/angular-jwt": "^1.1.0",
    "@chatie/graphql": "^0.4.8",
    "@types/blue-tape": "^0.1.31",
    "@types/glob": "^5.0.35",
    "apollo-codegen": "^0.19.0",
    "auth-angular": "^0.1.3",
    "auth0-js": "^9.4.2",
    "babel-eslint": "^8.2.2",
    "blue-tape": "^1.0.0",
    "brolog": "^1.2.15",
    "dotenv": "^5.0.1",
    "eslint": "^4.17.0",
    "git-scripts": "^0.2.1",
    "glob": "^7.1.2",
    "graphql-tag": "^2.8.0",
    "jwt-decode": "^2.2.0",
    "ng-packagr": "^2.4.2",
    "rxjs": "^5.5.6",
    "shx": "^0.2.2",
    "state-switch": "^0.4.4",
    "ts-node": "^5.0.1",
    "tsickle": "^0.27.5",
    "tslint": "^5.9.1",
    "tslint-jsdoc-rules": "^0.1.2",
    "typescript": "^2.7.1",
    "zone.js": "^0.8.25"
  },
  "peerDependencies": {
    "@angular/core": "^5.2.9",
    "@chatie/graphql": "^0.4.8",
    "auth-angular": "*",
    "brolog": ">=1",
    "graphql-tag": "^2.8.0",
    "jwt-decode": "^2.2.0",
    "rxjs": "^5.5.6",
    "state-switch": "^0.4.0"
  },
  "dependencies": {},
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    },
    "whitelistedNonPeerDependencies": []
  }
}
