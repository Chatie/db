{
  "name": "@chatie/db",
  "version": "0.3.0",
  "description": "Database for Chatie",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "shx rm -fr dist",
    "dist": "npm run clean && tsc && shx cp package.json dist/",
